{
    "name": "jolicode/gif-exception-bundle",
    "type": "symfony-bundle",
    "description": "The GhostBuster of your exception page",
    "keywords": ["symfony", "exception", "random", "gif"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Joli\\GifExceptionBundle\\": ""
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Joli\\GifExceptionBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4 || ^7.3 || ^8.0",
        "symfony/twig-bundle": "^6.4 || ^7.3 || ^8.0"
    },
    "require-dev": {
        "doctrine/annotations": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.17",
        "phpstan/phpstan": "^1.10",
        "spatie/image-optimizer": "^1.8",
        "symfony/asset": "^6.4 || ^7.3 || ^8.0",
        "symfony/browser-kit": "^6.4 || ^7.3 || ^8.0",
        "symfony/config": "^6.4 || ^7.3 || ^8.0",
        "symfony/console": "^6.4 || ^7.3 || ^8.0",
        "symfony/dependency-injection": "^6.4 || ^7.3 || ^8.0",
        "symfony/error-handler": "^6.4 || ^7.3 || ^8.0",
        "symfony/http-kernel": "^6.4 || ^7.3 || ^8.0",
        "symfony/monolog-bundle": "^3.8 || ^7.0",
        "symfony/phpunit-bridge": "^6.3 || ^7.0",
        "symfony/stopwatch": "^6.4 || ^7.3 || ^8.0",
        "symfony/web-profiler-bundle": "^6.4 || ^7.3 || ^8.0",
        "symfony/yaml": "^6.4 || ^7.3 || ^8.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
